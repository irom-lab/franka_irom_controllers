<?xml version="1.0" ?>
<launch>

  <arg name="stopped" default="false" />

  <rosparam command="load" file="$(find franka_irom_controllers)/config/franka_irom_controllers.yaml" />
  <node unless="$(arg stopped)" name="controller_spawner_cv" pkg="controller_manager" type="spawner" respawn="false" output="screen"  args="joint_velocity_node_controller"/>
  <node if="$(arg stopped)" name="controller_spawner_cv" pkg="controller_manager" type="spawner" respawn="false" output="screen"  args="--stopped joint_velocity_node_controller"/>

</launch>